<h2>Movie Look-up:</h3>
<h3 class="col-sm-3">Search for a movie:</h3>
<input type="text" class="col-sm-3 col-sm-offset-4" placeholder="enter movie name">
<hr>
<br>
<br>
<ul>
    @foreach( var entry in @ViewBag.history)
    {
        <li>@entry["name"]    @entry["rate"]  @entry["released_date"]</li>
    }
</ul>
