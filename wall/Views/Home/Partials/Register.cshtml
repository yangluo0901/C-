@model RegUser
<div class="container">
  <h1>Login</h1>
  <form class="form-horizontal" action="/register" method="post">
      <div class="form-group">
        <span asp-validation-for="@Model.first_name"></span>
        <label class="col-sm-2 control-label" for="first_name">First Name</label>
        <div class="col-sm-5">
          <input type="text" name="first_name" >
        </div>
      </div>

      <div class="form-group">
        <span asp-validation-for="@Model.last_name"></span>
        <label class="col-sm-2 control-label" for="last_name">Last Name</label>
        <div class="col-sm-5">
          <input type="text" name="last_name" >
        </div>
      </div>

      <div class="form-group">
        <span asp-validation-for="@Model.email"></span>
        <label class="col-sm-2 control-label" for="email">E-mail</label>
        <div class="col-sm-5">
          <input type="text" name="email" >
        </div>
      </div>

      <div class="form-group">
        <span asp-validation-for="@Model.password"></span>
        <label class="col-sm-2 control-label" for="password">Password</label>
        <div class="col-sm-5">
          <input type="text" name="password" >
        </div>
      </div>

      <div class="form-group">
        <label class="col-sm-2 control-label" for="con_password">Confirm Password</label>
        <div class="col-sm-5">
          <input type="text" name="con_password" >
        </div>
      </div>
      <div class="form-group">
        <input type="submit"  class="btn btn-success" value="Register">
      </div>
      @TempData["sucess"]
    </form>
  </div>