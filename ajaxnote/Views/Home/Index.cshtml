<div class="container">
  <h3>Notes:</h3>
  <hr>
  @{
    foreach(var note in @ViewBag.notes)
    {
      <h3 class = "col-sm-2">@note["title"]</h3>
      <p class="col-sm-2 col-sm-offset-4 text-important"> Delete </p>
      <p>note["content"]</p>
    }
  }
</div>

@Html.Partial("./Partials/AddNote")
@Html.Partial("./Partials/UpdateNote")
