@model DashboardModel
<form action="test/image" enctype="multipart/form-data" method="post">
    <div class="form-group">
        <div class="col-md-10">
            <p>Upload one or more files using this form:</p>
            <input type="file" name="images" multiple />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Upload" />
        </div>
    </div>
</form>


else
                    {
                        <div class="thumbnail">
                        <a href="/product/@product.product_id" class="col-sm-3">@product.name</a>   
                        </div>
                        
                    }