<!DOCtype=HTML>
<html>
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, intial-scale =1">
        <title>Dojo Survey Display</title>
        <!-- Latest compiled and minified CSS -->
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">

        <!-- jQuery library -->
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>

        <!-- Latest compiled JavaScript -->
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    </head>
    <body>
        <div class="container">
            <br><br>
            <div class="well well-lg">
                <u><h3>Submitted Info</h3></u>
                <p>Name:                   @ViewBag.Name</p>
                <p>Dojo Location:          @ViewBag.location</p>
                <p>Favorite Language:      @ViewBag.language</p>
                <p>Comment:                @ViewBag.comment</p>
           
                <a href="/" ><button type="button" class="btn btn-primary col-md-offset-2">Go Back</button></a>
                
            </div>
        </div>
    </body>
</html>