<!DOCType=HTML>
<html>
    <head>
        <meta charset="utf-8">
        <title>Random Passcode</title>
    </head>
    <body>
        <div class="Container">
            <h1>Random Passcode (passcode # @ViewBag.count)</h1>
            <textarea rows="1" cols="10">@ViewBag.passcode</textarea>
            <a href="/generate"><button class="btn btn-primary">Generate</button></a>
        </div>
    </body>
</html>